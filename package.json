{
	"name":        "vbb-translate-ids",
	"description": "Translate VBB ids old -> new & new -> old.",
	"version":     "2.0.0",
	"main":        "old-to-new.json",
	"files":       ["old-to-new.json", "new-to-old.json", "data.md", "vbb-logo.jpg"],
	"keywords":    ["vbb", "stations", "ids", "new", "ids", "international"],
	"author":      "Jannis R <mail@jannisr.de>",
	"homepage":    "https://github.com/derhuerst/vbb-translate-ids",
	"repository":  "derhuerst/vbb-translate-ids",
	"bugs":        "https://github.com/derhuerst/vbb-lines/issues-at",
	"license":     "ISC",
	"engines":     {"node": ">=6"},
	"devDependencies": {
		"xlsx-rows": "^1.0.0"
	},
	"scripts": {
		"download": "./download.sh",
		"cleanup": "rm data/old-to-new.xlsx",
		"build": "npm run download && node build.js && npm run cleanup",
		"test": "node test.js",
		"prepublish": "npm run build && npm test"
